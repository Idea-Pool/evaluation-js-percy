{
  "name": "evaluation-js-percy",
  "version": "0.0.1",
  "description": "This is a repository to create test-ware for evaluating Percy (JS) test automation tools.",
  "main": "-",
  "scripts": {
    "webdriver-update": "webdriver-manager update --gecko false --standalone false",
    "wd-init": "npm run webdriver-update || npm run webdriver-update",
    "percy": "percy exec -- protractor protractor.conf.js",
    "test": "npm run wd-init || npm run percy",
    "test-no-percy": "protractor protractor.conf.js",
    "lint": "eslint tests --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szikszail/evaluation-template.git"
  },
  "keywords": [
    "Test",
    "automation",
    "Tool",
    "Evaluation",
    "JavaScript"
  ],
  "author": "Idea Pool <SpecialEPM-DEBIdeaPoolStaff@epam.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/idea-pool/evaluation-js-percy/issues"
  },
  "homepage": "https://github.com/idea-pool/evaluation-js-percy#readme",
  "dependencies": {
    "@percy/protractor": "^1.1.0",
    "eslint": "^7.0.0",
    "husky": "^4.2.5",
    "protractor": "^7.0.0"
  },
  "husky": {
    "precommit": "npm run lint"
  }
}
